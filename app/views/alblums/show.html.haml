= render 'recent_alblums'

%ul#thumbnails.thumbnails
  - @alblum.photos.each do |photo|
    %li.span3
      = link_to photo.image.url(:large), :class => 'thumbnail' do
        = image_tag photo.image.url(:medium)

        %span.caption
          %i.zoom.icon-zoom-in
          %i.cart.icon-shopping-cart{'data-photo-id' => photo.id}
