= render 'album_list'

.span9.contentcolumn
  .row-fluid
    .span12
      .well
        .navbar.navbar-heading
          .navbar-inner
            .container{:style => "width: auto;"}
              %a.brand{:href => "#"}= @album.name

        = simple_form_for [:admin, @album],
          :html => {:multipart => true, :id => 'fileupload'} do |f|

          = render :partial => 'fileupload', :locals => {:f => f}

